{
  "expo": {
    "name": "QR Skanneri",
    "slug": "qrSkanneri",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/qrIcon.png",
    "splash": {
      "image": "./assets/qrSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "fi.flipgate.qrskanneri",
      "buildNumber": "1.1.0",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes."
      }
    },
    "android": {
      "icon": "./assets/qrIcon.png",
      "package": "fi.flipgate.qrskanneri",
      "versionCode": 1,
      "permissions": [
        "CAMERA"
      ]
    }

  }
}
